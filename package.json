{
  "name": "Inbox",
  "version": "0.1.0",
  "description": "A wrapper for Google Inbox",
  "scripts": {
    "start": "electron src/app.js",
    "build": "gulp build",
    "version": "gulp sync-package-files && git add src/package.json && gulp build",
    "postversion": "git push",
    "release": "gulp create-autoupdater-file && git add auto_updater.json && git commit -m 'Updates auto updater file' && git push && gulp release"
  },
  "repository": "github:martinlissmyr/deskbox",
  "keywords": [
    "desktop",
    "inbox",
    "mail",
    "wrapper"
  ],
  "author": "Martin Lissmyr",
  "license": "MIT",
  "devDependencies": {
    "electron-prebuilt": "0.36.7",
    "gulp": "~3.9.0",
    "gulp-util": "~3.0.7",
    "archiver": "~0.21.0",
    "electron-packager": "latest",
    "gulp-github-release": "latest",
    "dotenv": "~1.2.0",
    "del": "~2.0.0"
  }
}
